{
    "name": "mamitech/laravel-http-logger",
    "description": "Mamitech Laravel HTTP Logger",
    "keywords": [
        "mamitech",
        "laravel-http-logger"
    ],
    "homepage": "https://github.com/mamitech/laravel-http-logger",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Arifin",
            "email": "arifin@mamiteam.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11"
    },
    "require-dev": {
        "orchestra/testbench": "^9.5.0",
        "phpunit/phpunit": "^11.4.0"
    },
    "autoload": {
        "psr-4": {
            "Mamitech\\LaravelHttpLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mamitech\\LaravelHttpLogger\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mamitech\\LaravelHttpLogger\\HttpLoggerServiceProvider"
            ]
        }
    }
}
